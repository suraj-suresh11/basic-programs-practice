#include <iostream>

using namespace std;

int main()
{
    int a[5],max=0,i,min=0;
    for(i=0;i<5;i++)
    {
        cin>>a[i];
    }
    for(i=0;i<5;i++)
    {
        if(a[i]>max)
        {
            max=a[i];
        }
    }
    cout<<"max"<<max;
    
    for(i=0;i<5;i++)
    {
        if(a[i]<min)
        {
            min=a[i];
        }
    }
    cout<<"min"<<min;
    return 0;
}
