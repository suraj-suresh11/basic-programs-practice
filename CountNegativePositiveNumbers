#include <iostream>

using namespace std;

int main()
{
    int a[5];
    int neg=0,pos=0;
    for(int i=0;i<5;i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<5;i++)
    {
     if(a[i]<0)
     neg++;
     else if(a[i]>0)
     pos++;
    }
    cout<<"neg"<<neg<<endl;
    cout<<"pos"<<pos;
    return 0;
}
