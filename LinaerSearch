#include <iostream>

using namespace std;

int main()
{
    int a[5],key=0;
    for(int i=0;i<5;i++)
    {
        cin>>a[i];
    }
    cout<<"Enter search element"<<endl;
    cin>>key;
    for(int i=0;i<5;i++)
    {
        if(a[i]==key)
        {
            cout<<"element is found at"<<i+1;
            return 0;
        }
  
    }
    cout<<"element not found";
    
    return 0;
}
