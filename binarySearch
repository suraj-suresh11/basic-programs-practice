#include <iostream>

using namespace std;

int main()
{
    int a[5],key,mid,l=0,h=4;
    for(int i=0;i<5;i++)
    {
        cin>>a[i];
    }
    cout<<"Enter search element"<<endl;
    cin>>key;
    while(l<=h)
    {
        mid=(l+h)/2;
        if(key==a[mid])
        {
            cout<<"Found at"<<mid+1;
            return 0;
        }
        else if(key<a[mid])
        h=mid-1;
        else
        l=mid+1;
        
    }
    cout<<"element not found";
    
    return 0;
}
